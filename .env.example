# OCN Demo Environment Configuration
# Copy this file to .env and customize as needed

# OpenAI API Key (optional - leave empty for deterministic stub mode)
OPENAI_API_KEY=

# CloudEvent Subscriber URLs
ORCA_CE_SUBSCRIBER_URL=http://weave:8082/events
ORION_CE_SUBSCRIBER_URL=http://weave:8082/events

# Logging Configuration
LOG_LEVEL=info

# Demo Mode (set to true for demo environments)
DEMO_MODE=true
